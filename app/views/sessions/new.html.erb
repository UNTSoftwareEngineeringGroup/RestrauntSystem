<div class="login">
  <div class="container">
    <div class="form">

    <h1>Log in</h1>




<%= form_for(:session, url: guestlogin_path) do |f| %> 
  <%= f.email_field :email, :placeholder => "Email" %> 
  <%= f.password_field :password, :placeholder => "Password" %> 
  <%= f.submit "Submit", class: "btn-submit" %>
<% end %>